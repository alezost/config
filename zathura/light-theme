set statusbar-fg "#262626"

set index-bg "#ded6c5"
set index-fg "#262626"
set index-active-bg "#d9ceb2"
set index-active-fg "#262626"

set recolor-lightcolor "#ded6c5"
set recolor-darkcolor "#262626"
